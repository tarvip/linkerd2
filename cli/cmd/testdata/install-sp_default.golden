apiVersion: linkerd.io/v1alpha2
kind: ServiceProfile
metadata:
  name: linkerd-dst.linkerd.svc.cluster.local
  namespace: linkerd
spec:
  routes:
  - name: POST /io.linkerd.proxy.destination.Destination/Get
    condition:
      method: POST
